package selfstudy;

public class Notepad {

}
				 // 0 1 2 3 4
class Solution { // 1 2 3 4 5                  1        3
    public int[] solution(int[] numbers, int num1, int num2) {
        int[] answer = {}; // 2 3 4
        for(int i = num1; i <= num2; i++) {
        	answer= numbers[i];
        }
        
        return answer;
    }
}