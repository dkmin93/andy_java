package baekjoon04;

import java.util.Arrays;
import java.util.Scanner;

public class Quiz08 { //나머지

	public static void main(String[] args) {

		Scanner scan = new Scanner(System.in);
		int[] arr = new int[10]; // 10칸짜리 배열 생성
		int count = 10; // 카운팅할 변수 생성
		int repeat = 0; // 중복된 수

		for(int i = 0; i<10; i++) { //배열 입력 받기
			arr[i] = scan.nextInt();
			arr[i] = arr[i] % 42;

			//System.out.println(Arrays.toString(arr)); // 42로 나눈 나머지 체크


		}

		//int initial = arr[i];
		//if(initial != arr[i]) {
		//	count++;
		//}

		System.out.println(Arrays.toString(arr)); // 42로 나눈 나머지 체크


		for(int i = 0; i < 10; i++) { // 39
			int initial = arr[i]; // 39
			 for(int j = i+1; j < 10; j++) { //39를 40부터 비교
				 if(initial != arr[j]) {
					 repeat++;
				 } else if(initial == arr[j]) { //중복될경우 까야지
					 count--;
				 }
			 }
		}

		System.out.println(10-repeat);


	}

}
