		Person choi = new Employee("최강창민", 30, "가수");
		
		printPerson(hong);
		
		}
	
	public static void printPerson(Person p) {
		
		//p가 Student이면 Student 캐스팅
		//p가 Teacher이면 Teacher 캐스팅
		
	}

}
