package baekjoon02;

import java.util.Scanner;

public class Quiz07 { //주사위 세계
	
	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		
		int a = scan.nextInt();
		int b = scan.nextInt();
		int c = scan.nextInt();
		
		if(a == b && a == c) {
			System.out.println(10000 + a * 1000);
		}
		
	}

}
