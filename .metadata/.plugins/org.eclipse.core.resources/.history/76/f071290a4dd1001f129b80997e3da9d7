package day07.inherit.good;

public class MainClass {
	
	public static void main(String[] args) {
		
		Person p = new Person(); // 그렇게 많이 쓸 일은 없다
		p.name = "홍길동";
		p.age = 20;
		System.out.println(p.info());
		
		//상속받은 자식클래스들
		Student s = new Student();
		s.name = "이순신";
		s.age = 30;
		s.studentId = 123123
		System.out.println(s.info());
	}

}
