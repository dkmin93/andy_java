package day02;

public class DataTypeEx01 {
	public static void main(String[] args) {
		
		//정수형 타입
		byte a = 127; // 양수는 2의 7승(8-1) -1
		byte a2 = -128; // 음수는 - 2의 7승
		
		short b = 32767; // 양수는 2의 15승(16-1) -1
		short b2 = -32768;
		
		int c = 2147483647;
		int c2 = -2147483648;
		
		long d = 1213123123223;
	}
}
