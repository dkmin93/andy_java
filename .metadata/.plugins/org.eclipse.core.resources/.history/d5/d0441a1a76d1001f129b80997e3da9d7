package quiz04;

public class ToArray {
	
	void toArray(int[] arr) { // {1,2,3}
		System.out.print("[");
		for(int i = 0; i < arr.length; i++) {
			System.out.print(arr[i]);
			for(int j = 0; j < arr.length -1; j++) {
				System.out.print(", ");
			}
		}
		System.out.print("]");
	}
	
	void toArray(char[] arr) {
		for(int i = 0; i < arr.length; i++) {
			System.out.println(arr[i]);
		}
		
	}
	
	void toArray(String[] arr) {
		for(int i = 0; i < arr.length; i++) {
			System.out.println(arr[i]);
		}
		
	}
	

}
