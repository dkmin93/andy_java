package day06;

public class ObjectEx01 {
	
	public static void main(String[] args) {
		
		//객체를 사용하지 않는다면
		System.out.println("===1번 계산기===");
		System.out.println(add(10));
		System.out.println(add(20));
		System.out.println(add(30));
		
	}
	
	static int result =0;
	static int add(int a) {
		result += a;
		return result;
	}
	

}
