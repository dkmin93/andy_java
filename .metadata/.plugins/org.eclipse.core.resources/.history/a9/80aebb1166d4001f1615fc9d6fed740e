package selfstudy;

import java.util.Arrays;

public class Notepad {

}

class Solution {
    public int[] solution(int[] num_list) {
        int[] answer = {};
        int on = 0; //홀수
        int en = 0; //짝수
        for(int i = 0; i < num_list.length; i++) {
        	if(num_list[i] % 2 == 0) { //짝수
        		en++;
        	} else if(num_list[i] % 2 == 1) { //홀수
        		on++;
        	}
        }
        
        
        
        
        return answer;
    }
}

