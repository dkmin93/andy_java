package selfstudy;

import java.util.Arrays;

public class Notepad {

}

class Solution {
    public int solution(String[] s1, String[] s2) {
        int answer = 0;
        for(int i = 0; i < s1.length; i++) {
        		if(s1[i] == s2[i]) {
        			answer++;
        		}
        	
        }
        return answer;
    }
}