package day06;

public class Phone {

	String model;
	int price;
	String color;
	
	//생성자
	Phone() {
		System.out.println("폰 생성자 호출 됨!!!"); //
	}
	
	void info() {
		System.out.println("===휴대폰 정보===");
		System.out.println("모델:" + model);
		System.out.println("가격:" + price);
		System.out.println("색상:" + color);
	}
	
}
