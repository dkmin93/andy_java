package day02;

public class DataTypeEx02 {
	public static void main(String[] args) {
		
		//단일문자 저장하는 타입 char
		char c1 = 'A'; // 크기는 2byte
		char c2 = 65;
		System.out.println(c1);
		System.out.println(c2);
		
		char c3 = '가';
		char c4 = 44032;
		System.out.println(c3);
		System.out.println(c4);
		
	}
}
