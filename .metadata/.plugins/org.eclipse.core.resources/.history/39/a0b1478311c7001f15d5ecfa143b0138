package day01;

public class Variable {

	public static void main(String[] args) {
		
		/*
		 * 변수의 선언방법
		 * 데이터타입 이름;
		 * 
		 * int는 정수를 저장하는 대표적인 유형
		 * String은 문자열을 저장하는 대표적인 유형
		 * 
		 */
		
		int a; //변수의 선언
		a = 10; //초기화
		
		System.out.println(a);
		
		int a2 = 20; //변수의 선언과 초기화를 동시에도 할 수 있다.
		System.out.println(a2);
		
	}
}
