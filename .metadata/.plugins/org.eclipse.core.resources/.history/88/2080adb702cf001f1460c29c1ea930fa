package quiz02;

public class Account {
	
	/*
		
		출금기능 사용자에게 비밀번호를 입력받아서
		비밀번호가 일치하면 잔액을 차감해하고
		출금값을 반환하는 기능
		
		
		*/
	
	String name;
	String password;
	int money;
	
	Account() {
		
	}
	
	void deposit(int a) {
		money += a;
	}
	
	int withDraw(int a) { //출금기능
		money-=a;
		return a;
	}
		
		
	int getBalance() {
		return money;
	}
	

}
