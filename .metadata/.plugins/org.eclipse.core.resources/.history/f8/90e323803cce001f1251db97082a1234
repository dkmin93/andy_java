		int N = scan.nextInt(); //물건의 종류 갯수 입력
		int[] arr = new int[N];
		int[] arr2 = new int[N];
		
		for(int a = 0; a < N; a++) {
			
			arr[a] = scan.nextInt();
			arr2[a] = scan.nextInt();
			
	
		}
		
